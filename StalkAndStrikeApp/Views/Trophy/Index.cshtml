@model IEnumerable<StalkAndStrikeApp.Models.Hunter>

<h1>Hunters</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var hunter in Model)
        {
            <tr>
                <td>@hunter.Name</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@hunter.Id">Edit</a>
                    <a asp-action="Delete" asp-route-id="@hunter.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
